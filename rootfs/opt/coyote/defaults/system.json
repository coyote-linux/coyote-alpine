{
    "version": "4.0.0",
    "system": {
        "hostname": "coyote",
        "domainname": "local",
        "timezone": "America/New_York",
        "timeserver": "pool.ntp.org"
    },
    "users": [],
    "network": {
        "interfaces": [],
        "routes": [],
        "dns": {
            "nameservers": []
        }
    },
    "services": {
        "ssh": {
            "enabled": true,
            "port": 22,
            "allowed_hosts": []
        },
        "dhcpd": {
            "enabled": false,
            "interface": "lan",
            "range_start": "192.168.1.100",
            "range_end": "192.168.1.200",
            "lease_time": 86400
        },
        "dns": {
            "enabled": false,
            "forwarders": [
                "8.8.8.8",
                "8.8.4.4"
            ]
        },
        "snmp": {
            "enabled": false,
            "community": "public",
            "location": "",
            "contact": "",
            "allowed_hosts": []
        },
        "upnp": {
            "enabled": false,
            "interface": "lan",
            "external_interface": "wan",
            "secure_mode": true
        }
    },
    "firewall": {
        "enabled": true,
        "default_policy": "drop",
        "options": {
            "clamp_mss": null,
            "log_invalid": false
        },
        "logging": {
            "enabled": true,
            "prefix": "COYOTE",
            "level": "info",
            "local_deny": true,
            "forward_deny": true
        },
        "icmp": {
            "allow_ping": true,
            "allow_ping_reply": true,
            "rate_limit": null,
            "rate_burst": 5,
            "log_dropped": false,
            "ipv4": {
                "enabled": true,
                "types": []
            },
            "ipv6": {
                "enabled": true,
                "types": []
            }
        },
        "sets": {},
        "acls": [],
        "applied": [],
        "nat": {
            "bypass": [],
            "masquerade": []
        },
        "rules": [],
        "port_forwards": []
    },
    "vpn": {
        "ipsec": {
            "enabled": false,
            "tunnels": []
        }
    },
    "loadbalancer": {
        "enabled": false,
        "stats": {
            "enabled": true,
            "port": 8404,
            "uri": "/stats"
        },
        "defaults": {
            "mode": "http",
            "timeout_connect": "5s",
            "timeout_client": "50s",
            "timeout_server": "50s"
        },
        "frontends": [],
        "backends": []
    }
}
